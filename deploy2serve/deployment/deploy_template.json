{
  "torch_weights": "./weights/pytorch/model.pt",
  "formats": [
    "onnx",
    "tensorrt"
  ],
  "input_shape": [256, 192],
  "device": "cuda:0",
  "repeats": 1000,
  "enable_benchmark": true,
  "enable_visualization": true,

  "executor": {
    "module_path": "...",
    "class_name": "..."
  },
  "exporter": {
    "module_path": "...",
    "class_name": "..."
  },

  "tensorrt": {
    "plugins": [
      {
        "name": "...",
        "options": {
          "opt1": "val1"
        }
      }
    ],
    "specific": {
      "log_level": "INFO",
      "precision": "fp16",
      "profile_shapes": [
        {
          "min": [1, 3, 256, 192],
          "opt": [4, 3, 256, 192],
          "max": [8, 3, 256, 192]
        }
      ],
      "workspace": 268435456,
      "flags": [
        "TF32",
        "DIRECT_IO",
        "PREFER_PRECISION_CONSTRAINTS",
        "SPARSE_WEIGHTS"
      ],
      "profiling_verbosity": "NONE",
      "max_aux_streams": 4,
      "runtime_platform": null,
      "compatibility_level": null,
      "tactics": null,
      "algorithm": "ENTROPY_CALIBRATION_2"
    },
    "enable_timing_cache": true,
    "enable_calibration_cache": true,
    "output_file": "weights/tensorrt/model.engine",
    "force_rebuild": true
  },

  "onnx": {
    "specific": {
      "keep_initializers_as_inputs": false,
      "export_params": true,
      "verbose": null,
      "input_names": [
        "images"
      ],
      "output_names": [
        "output"
      ],
      "opset_version": 11,
      "dynamic_axes": {
        "images": {
          "0": "batch"
        },
        "output": {
          "0": "batch"
        }
      },
      "training": "EVAL",
      "do_constant_folding": true
    },
    "simplify": true,
    "output_file": "weights/onnx/model.onnx",
    "force_rebuild": true
  },

  "torchscript": {
    "optimize": false,
    "output_file": "weights/torchscript/model.pt",
    "force_rebuild": true
  },

  "openvino": {
    "precision": "fp16",
    "output_file": "weights/openvino/model.xml",
    "force_rebuild": true,
    "input_names": [
      "images"
    ],
    "device": "CPU"
  }
}
